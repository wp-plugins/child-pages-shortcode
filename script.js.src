(function(){
var $ = jQuery;
$.fn.equalHeights = function() {
    var currentTallest = 0;
    $(this).each(function(){
        if ($(this).height() > currentTallest) {
            currentTallest = $(this).height();
        }
    });
    $(this).each(function(){
        $(this).height(currentTallest);
    });
    return this;
};

$(".child_pages .child_page-container").equalHeights();
})(jQuery);
